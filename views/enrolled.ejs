<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title> seq association </title>
    <%- include partials/menubar %>
  </head>
  <body>
    <h1><%= panggilData[0].Subject.subject_name%></h1>
      <a href="/subjects">Return to Subject List</a>
      <table>
        <thead>
          <tr>
            <th>Name </th>
            <th>Score </th>
            <th>  Score Letter </th>
          </tr>
        </thead>
        <tbody>
          <% var rows = 0 %>
          <% panggilData.forEach(student =>{%>
            <tr>
              <td><%= student.Student.first_name +' '+ student.Student.last_name%></td>
              <% if(student.Score == null){ %>
                <td><a href="/subjects/<%=student.StudentId%>/givescore/<%=student.SubjectId%>">Give Score</a></td>
              <%}else{%>
                <td><%=student.Score%></td>
              <%}%>
              <td> <%= panggilGrade[rows] %></td>
              <% rows++ %>
            </tr>
          <%})%>
        </tbody>
      </table>
  </body>
</html>
