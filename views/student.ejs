<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel='stylesheet' href='style.css' />
    <title> <%= pageTitle %> </title>
    <%- include partials/menubar %>
  </head>
  <body>
    <br>
    <br>
    <h1> Students Data </h1>
    <a href="students/add"> Add Student </a>
    <table>
      <thead>
        <tr>
          <th> No </th>
            <th> First Name </th>
            <th> Last Name </th>
            <th> Email </th>
            <th> Jurusan </th>
            <th> Action </th>
          <th> Options </th>
          </tr>
        </thead>
      <tbody>
        <% let number = 1 %>
        <% if(panggilData) {panggilData.forEach(function(student) { %>
      <tr>
      <th> <%= number++ %> </th>
      <th> <%= student.first_name %> </th>
      <th> <%= student.last_name %> </th>
      <th> <%= student.email %> </th>
      <th> <%= student.jurusan %> </th>
      <th><a href="/students/<%= student.id %>/addsubject">Add Subject</a></th>
      <th><a href="/students/edit/<%= student.id %>">Edit</a></th>
      <th> <a href="/students/delete/<%= student.id %>">Delete</a> </th>
      </tr>
        <%  })} %>
      </tbody>
    <table>
<br>
<br>
<br>
  </body>
</html>
